{
	"title": "posts",
	"_processors": [{
		"name": "getIndex",
		"options": {
			"property": "_links.profile.href",
			"filter": "/schema/post",
			"to": "_links.item"
		}
	}, {
		"name": "sort",
		"options": {
			"property": "_links.item",
			"key": "date-created",
			"compare": "date",
			"descending": true
		}
	}, {
		"name": "embed",
		"options": {
			"property": "_links.item",
			"pick": ["date-created", "excerpt", "excerptHtml"]
		}
	}]
}
