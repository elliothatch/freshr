{
    "title": "tags",
	"_processors": [{
		"name": "getIndex",
		"options": {
			"property": "tag",
			"to": "_links.item"
		}
	}, {
		"name": "embed",
		"options": {
			"property": "_links.item",
			"pick": ["_links.item"]
		}
	}, {
		"name": "map",
		"options": {
			"property": "_embedded.item",
			"processor": {
				"name": "embed",
				"options": {
					"property": "_links.item",
					"pick": ["date-created"]
				}
			}
		}
	}]
}
