<div>Links:
	<ul>
		{{#each _links}}
		<li>{{@key}}<ul>
				{{#if (isArray this)}}
				{{#each this}}
				<li>{{hal-link @../key this}}</li>
				{{/each}}
				{{else}}
				<li>{{hal-link @key this}}</li>
				{{/if}}
		</li></ul>
		{{/each}}
	</ul>
	{{#each this}}
	<div>
		{{#unless (startsWith @key '_')}}
			<h2>{{@key}}</h2>
			<div>{{this}}</div>
		{{/unless}}
	</div>
	{{/each}}
</div>
